{
  "compilerOptions": {
    "module": "commonjs",     
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit":false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx":"preserve",
    "experimentalDecorators": true,
    "strict": false,                
    "baseUrl": "./",                
    "rootDir":".",
    "paths": {},                    
    "esModuleInterop": true,
    "skipLibCheck":true,
    "inlineSourceMap": true,        
    "inlineSources": true,          
  },
  "include":[
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue"
  ],
  "exclude":[
    "node_modules"
  ]
}